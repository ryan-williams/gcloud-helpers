
alias dcc="gcloud dataproc clusters create"
alias dcd="gcloud dataproc clusters delete"
alias dcl="gcloud dataproc clusters list"
alias dcs="gcloud dataproc clusters describe"
alias dcu="gcloud dataproc clusters update"

export CLOUDSDK_PYTHON=python

# The next line updates PATH for the Google Cloud SDK.
if [ -f "$HOME/google-cloud-sdk/path.bash.inc" ]; then
  source "$HOME/google-cloud-sdk/path.bash.inc"
fi

# The next line enables shell command completion for gcloud.
if [ -f "$HOME/google-cloud-sdk/completion.bash.inc" ]; then
  source "$HOME/google-cloud-sdk/completion.bash.inc"
fi

alias gcppi="gcloud projects get-iam-policy"
alias gcpcgc="gcloud container clusters get-credentials"
alias gcplr="gcloud beta iam roles list"

